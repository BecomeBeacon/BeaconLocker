<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="223dp"
        app:srcCompat="@drawable/item_notebook" />

    <TextView
        android:id="@+id/et_NICKNAME"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="testNICKNAME"
        android:layout_marginBottom="13dp"
        android:layout_above="@+id/et_Address"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/et_Address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="B9407F30-F5F8-466E-AFF9-25556B57FE6D"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/editText2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="center_horizontal"
        android:text="거리(m 단위)"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/et_Address"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/meter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="center_horizontal"
        android:text="40"
        android:textSize="42sp"
        android:layout_below="@+id/editText2"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button_fromItem_toMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="메인으로"
        android:layout_marginBottom="70dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>