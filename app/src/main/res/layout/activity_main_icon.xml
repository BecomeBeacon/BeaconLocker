<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_google_signin_btn_text_dark_default"
    android:orientation="vertical">

    <Button
        android:id="@+id/button14"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="152dp"
        android:background="@drawable/ic_menu_machine"
        android:backgroundTint="@color/colorButton" />

    <Button
        android:id="@+id/button15"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignBottom="@+id/button14"
        android:layout_alignTop="@+id/button14"
        android:layout_toEndOf="@+id/button14"
        android:layout_toRightOf="@+id/button14"
        android:background="@drawable/ic_button_person"
        android:backgroundTint="@color/colorButton" />

    <Button
        android:id="@+id/button16"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignLeft="@+id/button14"
        android:layout_below="@+id/button14"
        android:background="@drawable/ic_place"
        android:backgroundTint="@color/colorButton" />

    <Button
        android:id="@+id/button17"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@+id/button15"
        android:layout_toEndOf="@+id/button14"
        android:background="@drawable/ic_menu_laf"
        android:backgroundTint="@color/colorButton" />
</RelativeLayout>